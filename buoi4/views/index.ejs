<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Person</title>
</head>
<body>
    <h1>List Person</h1>
    <a href="/add">ADD PERSON</a>
    <table border="1" cellpadding="10">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Country</th>
        </tr>
            <% persons.forEach(person => { %>
        <tr>
            <td><%= person.name %></td>
            <td><%= person.age %></td>
            <td><%= person.country %></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>